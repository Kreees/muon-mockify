{
  "name": "muon-mockify",
  "version": "0.0.1",
  "description": "The easiest way to mockify your project",
  "main": "main.js",
  "scripts": {
    "pretest": "rm -rf reports || true && mkdir reports",
    "test": "./node_modules/.bin/mocha -C --reporter xunit > reports/xunit.xml",
    "posttest": "chmod 777 reports/xunit.xml"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.muonapps.com/muon-group/muon-mockify.git"
  },
  "keywords": [
    "mock",
    "test",
    "mockify",
    "debug"
  ],
  "author": "Sukharev Kirill <SukharevKirill@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "mocha": "^2.1.0"
  }
}
