{
  "name": "muon-mockify",
  "version": "0.0.1",
  "description": "The easiest way to mockify your project",
  "main": "main.js",
  "scripts": {
    "pretest": "rm -rf reports || true && mkdir reports",
    "test": "./node_modules/.bin/mocha -C --reporter xunit > reports/xunit.xml || true",
    "posttest": "chmod 777 reports/xunit.xml",
    "presystemtest": "npm test",
    "systemtest": "cd systemtest && ../node_modules/.bin/mocha -C --reporter xunit > ../reports/xunit-system.xml || true && cd ..",
    "postsystemtest": "chmod 777 reports/xunit.xml"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.muonapps.com/muon-group/muon-mockify.git"
  },
  "keywords": [
    "mock",
    "test",
    "mockify",
    "debug"
  ],
  "author": "Sukharev Kirill <SukharevKirill@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^1.10.0",
    "coffee-script": "^1.8.0",
    "mocha": "^2.1.0",
    "sinon": "^1.12.2"
  },
  "dependencies": {
    "stacktrace-js": "^0.6.4",
    "underscore": "^1.7.0",
    "walk": "^2.3.9"
  }
}
